{
    "version" : 2,
    "buildCommand": "npm run vercel-build",
    "builds" : [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "files": [
        {
          "output": "cliente/build"
        }
      ]

}